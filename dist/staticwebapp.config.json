{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/assets/*",
      "/*.{png,jpg,gif,svg,ico}",
      "/*.{css,scss}",
      "/*.js"
    ]
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=604800, immutable"
      }
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/about",
      "rewrite": "/index.html"
    },
    {
      "route": "/contact",
      "rewrite": "/index.html"
    },
    {
      "route": "/services",
      "rewrite": "/index.html"
    },
    {
      "route": "/services/integration-support",
      "rewrite": "/index.html"
    },
    {
      "route": "/services/emissions-reduction",
      "rewrite": "/index.html"
    },
    {
      "route": "/services/technology-consulting",
      "rewrite": "/index.html"
    },
    {
      "route": "/california-hydrogen-potential",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}