{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{png,jpg,gif,svg,PNG,JPG,GIF,SVG}",
      "*.{css,js}",
      "/assets/*",
      "/data/*"
    ]
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval'; img-src 'self' data: blob: https:; style-src 'self' 'unsafe-inline';"
  }
}